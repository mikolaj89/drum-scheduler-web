{
  "name": "@drum-scheduler/db-schema",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./relations": {
      "types": "./dist/relations.d.ts",
      "default": "./dist/relations.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "db:pull": "drizzle-kit pull --config drizzle.config.cjs"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.38.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "drizzle-kit": "0.30.2",
    "drizzle-orm": "^0.38.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  }
}
